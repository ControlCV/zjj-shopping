<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.sanda.dao.GoodsDao">

    <resultMap id="BaseResultMap" type="cn.edu.sanda.entity.Goods">
        <!--@Table goods-->
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="goodsName" column="goods_name" jdbcType="VARCHAR"/>
        <result property="goodsComment" column="goods_comment" jdbcType="VARCHAR"/>
        <result property="goodsPrice" column="goods_price" jdbcType="NUMERIC"/>
        <result property="goodsPic" column="goods_pic" jdbcType="VARCHAR"/>
        <result property="goodsStatus" column="goods_status" jdbcType="INTEGER"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="createUser" column="create_user" jdbcType="VARCHAR"/>
        <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
        <result property="updateUser" column="update_user" jdbcType="VARCHAR"/>
    </resultMap>


</mapper>